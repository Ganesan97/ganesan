import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
Scanner sc=new Scanner(System.in);
String a=sc.nextLine();
char c=sc.next().charAt(0);
char b[]=new char[a.length()];
b=a.toCharArray();
for(int i=0;i<a.length();i++)
{
    if(b[i]!=c)
    {
        System.out.print(b[i]);
    }
    else
    {
        break;
    }
}


	}
}
